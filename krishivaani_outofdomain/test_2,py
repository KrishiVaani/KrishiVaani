import pandas as pd
df = pd.read_csv("/workspace/abhinav/krishivaani_inferences/outofdomain_inferences/OutofDomain_new.csv")

# Load filenames from the text file
file_path = '/workspace/abhinav/krishivaani_inferences/outofdomain_filter_inferences/OutOfDomain.txt'
with open(file_path, 'r', encoding='utf-8') as file:
    filenames = [line.split('\t')[0].replace('.wav', '') for line in file if line.strip()]

# Filter the DataFrame based on filenames
filtered_df = df[df['filename'].isin(filenames)]

# Display the filtered DataFrame
print(filtered_df)

# Optionally, save the filtered DataFrame
filtered_df.to_csv('/workspace/abhinav/krishivaani_inferences/outofdomain_filter_inferences/OutofDomain_filtered.csv')
